  <div class="container">
    {{!-- post artciles that are saved here --}}
    <div id="listOfArticles">
      <h3>Saved Articles</h3>
      <ul id="list-of-articles" class="list-group">
        {{#each articles}}
        <li data-id="{{this.id}}" class="list-group-item">
                    <img id="flower" src="/assets/images/flower2.png">

          <img id="articleImg" src={{this.img}}>

          <h5 data-id="{{this.id}}">{{this.title}} </h5>
          <p>{{description}}</p>
                    <P>{{this.link}}</P>

          <button id="deleteBtn" data-id="{{this.id}}" class="btn orange darken-4">ï½˜</button>
          <button id="note" class="btn orange darken-1">Note</button>
          {{!-- <div id="notes"></div> --}}
        </li>
        {{/each}}
      </ul>
    </div>

    <!-- Modal Structure -->
    <div id="modal1" class="modal">
      <div class="modal-content">
        <div id="notes"></div>
        <div id="oldNotes"></div>
      </div>
      <div class="modal-footer">
       </div>
    </div>

